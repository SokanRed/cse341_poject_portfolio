    "paths": {
        "/vehicle": {
            "get": {
                "tags": ["vehicle"],
                "description": "Get all vehicles",
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "200": {
                        "description": "OK"
                    },
                    "500": {
                        "description": "Internal Server Error"
                    }
                }
            },
            "post": {
                "tags": ["vehicle"],
                "description": "Created vehicle",
                "parameters": [{
                    "name": "body",
                    "in": "body",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "brand": {
                                "example": "Ford"
                            },
                            "model": {
                                "example": "Mustang Shelby GT500 Code Red"
                            },
                            "horsePower": {
                                "example": "1300"
                            },
                            "fuelType": {
                                "example": "gas"
                            },
                            "color": {
                                "example": "Black"
                            },
                            "category": {
                                "example": "Sport"
                            },
                            "transmission": {
                                "example": "Automatic"
                            },
                            "numberPassenger": {
                                "example": "4"
                            }
                        }
                    }
                }],
                "responses": {
                    "201": {
                        "description": "Created"
                    },
                    "422": {
                        "description": "Unprocessable Entity"
                    },
                    "500": {
                        "description": "Internal Server Error"
                    }
                }
            }
        },
        "/vehicle/{id}": {
            "get": {
                "tags": ["vehicle"],
                "description": "Get vehicle by Id",
