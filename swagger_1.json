                "parameters": [{
                        "name": "id",
                        "in": "path",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "body",
                        "in": "body",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "brand": {
                                    "example": "Mercedes"
                                },
                                "model": {
                                    "example": "MayBach"
                                },
                                "horsePower": {
                                    "example": "750"
