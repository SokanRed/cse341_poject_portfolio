                        "description": "OK"
                    }
                }
            },
            "post": {
                "tags": ["maintenance"],
                "description": "Created maintenance",
                "parameters": [{
                    "name": "body",
                    "in": "body",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "vehicle": {
                                "example": "Ford Mustang"
                            },
                            "date": {
                                "example": "2023/10/10"
                            },
                            "mileage": {
                                "example": "10 000"
                            },
                            "lubrification": {
                                "example": "done"
                            },
                            "brakes": {
                                "example": "Next time"
                            },
                            "mirrors": {
                                "example": "okay"
                            },
                            "tires": {
                                "example": "Next time"
                            },
                            "suspensionSystem": {
                                "example": "okay"
                            }
                        }
                    }
                }],
                "responses": {
                    "201": {
                        "description": "Created"
                    },
                    "500": {
                        "description": "Internal Server Error"
                    }
                }
            }
