        },
        "/vehicle/": {
            "get": {
                "tags": ["vehicle"],
                "description": "Get all vehicles",
                "produces": [
                    "application/json"
                ],
                "parameters": [],
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            },
            "post": {
                "tags": ["vehicle"],
                "description": "Created vehicle",
                "parameters": [{
                    "name": "body",
                    "in": "body",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "brand": {
                                "example": "Ford"
                            },
                            "model": {
                                "example": "Mustang Shelby GT500"
                            },
                            "horsePower": {
                                "example": "1000"
                            },
                            "fuelType": {
                                "example": "gas"
                            },
                            "color": {
                                "example": "Black"
                            },
                            "category": {
                                "example": "Sport"
                            },
                            "transmission": {
                                "example": "Automatic"
                            },
                            "numberPassenger": {
                                "example": "4"
                            }
                        }
                    }
                }],
                "responses": {
                    "201": {
                        "description": "Created"
                    },
                    "500": {
                        "description": "Internal Server Error"
                    }
                }
            }
        },
        "/vehicle/{id}": {
            "get": {
                "tags": ["vehicle"],
                "description": "Get vehicle by Id",
                "produces": [
                    "application/json"
                ],
                "parameters": [{
                    "name": "id",
                    "in": "path",
                    "required": true,
                    "type": "string"
                }],
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
        "delete": {
            "tags": ["vehicle"],
            "description": "Delete Vehicle",
            "parameters": [{
                "name": "id",
                "in": "path",
                "required": true,
                "type": "string"
            }],
            "responses": {
                "200": {
                    "description": "Deleted"
                },
                "500": {
                    "description": "Internal Server Error"
                }
            }
        },
        "/maintenance/": {
            "get": {
                "tags": ["maintenance"],
                "description": "Get all maintenance",
                "produces": [
