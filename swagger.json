            "delete": {
                "tags": ["maintenance"],
                "description": "Delete Maintenance",
                "parameters": [{
                    "name": "id",
                    "in": "path",
                    "required": true,
                    "type": "string"
                }],
                "responses": {
                    "200": {
                        "description": "Deleted"
                    },
                    "500": {
                        "description": "Internal Server Error"
                    }
                }
